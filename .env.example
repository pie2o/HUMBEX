# TradingView webhook secret (generate with: openssl rand -hex 32)
TRADINGVIEW_SECRET=your_tradingview_secret_here

# AES-GCM encryption key for storing API keys (generate with: openssl rand -hex 32)
ENCRYPTION_KEY_HEX=your_encryption_key_hex_here

# Supabase/PostgreSQL connection string
DATABASE_URL=postgresql://user:password@localhost:5432/humbex

# Backend settings
BACKEND_PORT=8000
BACKEND_HOST=0.0.0.0

# Worker settings
WORKER_POLL_INTERVAL=5

# CCXT settings (dry-run mode by default)
CCXT_TEST_MODE=true
